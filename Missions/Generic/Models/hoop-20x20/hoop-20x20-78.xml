<?xml version="1.0"?>

<PropertyList>
  <path type="string">hoop-20x20.ac</path>
  <animation>
    <object-name type="string">hoop-red</object-name>
    <object-name n="1" type="string">hoop-green</object-name>
    <enable-hot type="string">false</enable-hot>
  </animation>
  <animation n="1">
    <type type="string">select</type>
    <object-name type="string">hoop-red</object-name>
    <condition>
      <property type="string">/sim/mission/objects/hoop[78]/finished</property>
    </condition>
  </animation>
  <animation n="2">
    <type type="string">select</type>
    <object-name type="string">hoop-green</object-name>
    <condition>
      <not>
        <property type="string">/sim/mission/objects/hoop[78]/finished</property>
      </not>
    </condition>
  </animation>
</PropertyList>
