<?xml version="1.0"?>

<PropertyList>
	<path>marker-d20.ac</path>
	<animation>
		<type>rotate</type>
		<object-name>circle</object-name>
		<use-personality type="bool">true</use-personality>
		<property>/sim/time/elapsed-sec</property>
		<factor>30</factor>
		<axis>
			<x>0.0</x>
			<y>0.0</y>
			<z>1.0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
	</animation>
	<animation>
		<object-name>circle</object-name>
		<enable-hot type="bool">false</enable-hot>
	</animation>
</PropertyList>
