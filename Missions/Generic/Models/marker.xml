<?xml version="1.0"?>

<PropertyList>
  <path>marker.ac</path>

  <animation>
    <type>billboard</type>
    <object-name>Plane</object-name>
    <spherical type="bool">false</spherical>
  </animation>

  <animation>
	<type>dist-scale</type>
	<object-name>Plane</object-name>
	<interpolation>

      <entry>
		<ind>1</ind>
		<dep>0.05</dep>
	  </entry>

      <entry>
		<ind>50</ind>
		<dep>0.15</dep>
	  </entry>

	  <entry>
		<ind>100</ind>
		<dep>0.5</dep>
	  </entry>
	  <entry>
		<ind>10000</ind>
		<dep>10</dep>
	  </entry>
	  <entry>
		<ind>30000</ind>
		<dep>15</dep>
	  </entry>
	  <entry>
		<ind>50000</ind>
		<dep>20</dep>
	  </entry>
	  <entry>
		<ind>100000</ind>
		<dep>50</dep>
	  </entry>

	</interpolation>
  </animation>


  <animation>
	<object-name>Plane</object-name>
	<enable-hot type="bool">false</enable-hot>
  </animation>

  <animation>
    <type>material</type>
    <shininess>0</shininess>
    <specular>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
    </specular>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
    </emission>
    <ambient>
      <red>1.0</red>
      <green>1.0</green>
      <blue>1.0</blue>
    </ambient>
    <diffuse>
      <red>1.0</red>
      <green>1.0</green>
      <blue>1.0</blue>
    </diffuse>
    <object-name>Plane</object-name>
  </animation>

</PropertyList>
